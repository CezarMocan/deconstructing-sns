{
  "name": "Deconstructing SNS",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Try visiting Facebook and watch everything burn.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*",
        "https://www.gsp.ro/*"
      ],
      "js": [
        "lib/jquery-1.11.1.min.js",
        "lib/p5.min.js",
        "lib/p5.sound.js",
        "lib/tone.js",
        "inject.js"
      ]
    }
  ],
  "permissions": ["storage"]
}